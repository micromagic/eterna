<!--
	mysql的类型定义
-->
<eterna-config>
	<factory>

		<include src="impl/config_MySQL.xml"/>

		<objs>

			<constant name="int" value="INT" />
			<constant name="short" value="SMALLINT" />
			<constant name="byte" value="TINYINT" />
			<constant name="long" value="BIGINT" />
			<constant name="double" value="DOUBLE" />
			<constant name="String" value="VARCHAR" />
			<constant name="Datetime" value="TIMESTAMP" />
			<constant name="Blob" value="BLOB" />
			<constant name="Clob" value="TEXT" />

			<constant name="now" value="now()" />
			<constant name="now_script" value="select now()" />

			<constant name="rename" value="rename" />

		</objs>
	</factory>
</eterna-config>
